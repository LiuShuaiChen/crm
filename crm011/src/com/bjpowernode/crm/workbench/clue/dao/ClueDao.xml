<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bjpowernode.crm.workbench.clue.dao.ClueDao">

	<!-- int createClue(Clue clue); -->
	<insert id="createClue" parameterType="Clue">
		insert into tbl_clue
		(id,owner,company,phone,website,grade,industry,annualIncome,empNums,country,province,city,street,zipcode,description,fullName,appellation,source,email,mphone,job,state,createBy,createTime,editBy,editTime,contactSummary,nextContactTime)
		values
		(#{id},#{owner},#{company},#{phone},#{website},#{grade},#{industry},#{annualIncome},#{empNums},#{country},#{province},#{city},#{street},#{zipcode},#{description},#{fullName},#{appellation},#{source},#{email},#{mphone},#{job},#{state},#{createBy},#{createTime},#{editBy},#{editTime},#{contactSummary},#{nextContactTime})
	</insert>


</mapper>




